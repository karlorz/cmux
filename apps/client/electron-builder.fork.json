{
  "extends": "./electron-builder.json",
  "appId": "com.karlorz.cmux",
  "productName": "cmux-next",
  "protocols": [
    {
      "name": "cmux-next",
      "schemes": [
        "cmux-next"
      ]
    }
  ],
  "extraResources": [
    {
      "from": "assets/cmux-logos/cmux.iconset",
      "to": "cmux-logos/cmux.iconset"
    },
    {
      "from": "electron/app-update.next.yml",
      "to": "app-update.yml"
    },
    {
      "from": "../../apps/server/native/core",
      "to": "native/core",
      "filter": [
        "*.node"
      ]
    }
  ]
}
